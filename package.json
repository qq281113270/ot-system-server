    "scripts": {
        "//注释1": "webpack编译node源码",
        "build:dev": "cross-env NODE_ENV=development       babel-node ./scripts/webpack/index.js   -r  dotenv/config  dotenv_config_path=.env.development",
        "build:prd": "cross-env NODE_ENV=production        babel-node ./scripts/webpack/index.js   -r  dotenv/config  dotenv_config_path=.env.production",
        "build:dev:test": "cross-env NODE_ENV=development  babel-node ./scripts/webpack/index.js  webpackEnv=test",
        "build:prd:test": "cross-env NODE_ENV=production   babel-node ./scripts/webpack/index.js  webpackEnv=test",
        "//注释2": "webpack编译node源码，并且启动node程序",
        "start": "npm run build:dev&npm run dev:pm2&npm run pm2log",
        "pm2log": "pm2 logs",
        "//注释3": "用pm2启动node程序",
        "dev": "npm run dev:p  -r  dotenv/config  dotenv_config_path=.env.development",
        "dev:p": "cross-env NODE_ENV=development  pm2 start ./dist/index.js --watch  -r   dotenv/config  dotenv_config_path=.env.development",
        "test:p": "cross-env NODE_ENV=production  pm2 start ./dist/index.js --watch",
        "prd:p": "cross-env NODE_ENV=production   pm2 start ./dist/index.js --watch  -r  dotenv/config  dotenv_config_path=.env.production",
        "//注释4": "用nodemon启动node程序",
        "dev:n": "nodemon    --exec cross-env NODE_ENV=development     babel-node ./dist/index.js    -r  dotenv/config  dotenv_config_path=.env.development",
        "test:n": "nodemon --exec cross-env NODE_ENV=production    babel-node ./dist/index.js  -r  dotenv/config  dotenv_config_path=.env.development",
        "prd:n": "nodemon  --exec cross-env NODE_ENV=production    babel-node ./dist/index.js  -r  dotenv/config  dotenv_config_path=.env.production",
        "//注释8单元测试命令": "运行单元测试",
        "test": "npm run jest",
        "//注释9": "生成测试覆盖率报告",
        "coverage": "jest --coverage --config jest.config.ts"
    },
